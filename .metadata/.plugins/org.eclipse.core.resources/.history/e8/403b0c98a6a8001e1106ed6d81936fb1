package com.takeo;

public class SpringBean {
	static int i;
	
	public SpringBean() {
		System.out.println((++i)+")Executed the SpringBean class constructor.................");
	}
	
	private String name;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	
	@Override
	public String getString() {
		return name;
	}

}
